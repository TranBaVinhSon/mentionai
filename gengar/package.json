{
  "name": "gengar",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_OPTIONS=--max-old-space-size=6144 nest build",
    "heroku-postbuild": "NODE_OPTIONS=--max-old-space-size=6144 npm run prebuild && NODE_OPTIONS=--max-old-space-size=6144 nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --expose-gc dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "update": "git checkout main && git pull origin main && yarn && yarn build && yarn migration:run",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "seed:run": "typeorm-seeding seed -d ./dist/ormconfig.js ./dist/src/db/seeds/*.seeder.js",
    "migration:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create",
    "migration:seed:run": "yarn migration:run && yarn seed:run",
    "migration:run": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:run -d dist/ormconfig.js",
    "migration:revert": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert -d dist/ormconfig.js",
    "command:run": "ts-node -r tsconfig-paths/register src/console.ts",
    "command:run:gc": "node --expose-gc -r ts-node/register -r tsconfig-paths/register src/console.ts",
    "command:generate-metadata": "ts-node -r tsconfig-paths/register src/console.ts generate-metadata",
    "command:generate-embeddings:gc": "node --expose-gc -r ts-node/register -r tsconfig-paths/register src/console.ts generate-embeddings",
    "command:generate-embeddings:prod": "./run-command.sh generate-embeddings",
    "command:generate-metadata:prod": "./run-command.sh generate-metadata",
    "command:generate-about": "ts-node -r tsconfig-paths/register src/console.ts generate-about",
    "command:generate-knowledge-graph": "ts-node -r tsconfig-paths/register src/console.ts generate-knowledge-graph",
    "command:reindex-linkedin-memories": "ts-node -r tsconfig-paths/register src/console.ts reindex-linkedin-memories",
    "command:reindex-linkedin-memories:gc": "node --expose-gc -r ts-node/register -r tsconfig-paths/register src/console.ts reindex-linkedin-memories",
    "heroku:generate-metadata": "node dist/src/console.js generate-metadata",
    "heroku:generate-embeddings": "node dist/src/console.js generate-embeddings",
    "heroku:reindex-linkedin-memories": "node --expose-gc dist/src/console.js reindex-linkedin-memories"
  },
